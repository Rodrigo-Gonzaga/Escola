  /**
 * showToastMessage shows toast message 
 *
 * @param {string} message text message
 * @param {string} type type of message, error red and success green message
 * @param {number} duration of toast to be visible
 */
 const showToastMessage = (
    message,
    type = "success",
    duration = 3500,
) => {
    if (message) {
        const node = document.createElement("DIV");
        const textnode = document.createTextNode(message);
        node.appendChild(textnode);
        node.classList.add("custom-toast-wrapper");
        document.body.appendChild(node);
        const el = document.querySelector(".custom-toast-wrapper");

        setTimeout(() => {
            const el = document.querySelector(".custom-toast-wrapper");
            if (el) el.remove();
        }, duration);

        switch (type) {
            case "success":
                el.classList.add("success-message");
                break;
            case "error":
                el.classList.add("error-message");
            break;
        }
    }
}

