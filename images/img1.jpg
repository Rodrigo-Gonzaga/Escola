<script>
    var currentIndex = 0;
    var listLength = 5;
    var changeCarouselDataWithoutIndex = function (state) {
        var image = document.getElementsByClassName("wh-superstars__image-wrap")[0];
        var imageData = document.getElementsByClassName("wh-superstars__content-wrap")[0];
        var navBtns = document.getElementsByClassName("wh-superstars__navigation-btns")[0];
        image.children[currentIndex].classList.value = "wh-superstars__img-content";
        imageData.children[currentIndex].classList.value = "wh-superstars__content";
        navBtns.children[currentIndex].classList.value = "wh-superstars__navigation-btn";
        if (state === "next" && currentIndex < listLength - 1) {
            currentIndex++;

        } else if (state === "prev" && currentIndex <= listLength - 1) {
            currentIndex === 0 ? currentIndex = listLength - 1 : currentIndex--;
        } else {
            currentIndex = 0;
        }
        image.children[currentIndex].classList.value = "wh-superstars__img-content active";
        imageData.children[currentIndex].classList.value = "wh-superstars__content active";
        navBtns.children[currentIndex].classList.value = "wh-superstars__navigation-btn active";
    }
    var changeCarouselDataWithIndex = function (index) {
        var image = document.getElementsByClassName("wh-superstars__image-wrap")[0];
        var imageData = document.getElementsByClassName("wh-superstars__content-wrap")[0];
        var navBtns = document.getElementsByClassName("wh-superstars__navigation-btns")[0];
        for (var i = 0; i < listLength; i++) {
            image.children[i].classList.value = "wh-superstars__img-content";
            imageData.children[i].classList.value = "wh-superstars__content";
            navBtns.children[i].classList.value = "wh-superstars__navigation-btn";
        }
        image.children[index].classList.value = "wh-superstars__img-content active";
        imageData.children[index].classList.value = "wh-superstars__content active";
        navBtns.children[index].classList.value = "wh-superstars__navigation-btn active";
        currentIndex = index;
    }
</script>
