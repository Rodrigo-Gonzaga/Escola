  function mainTabChange(learningType) {
    var learningTypeChildTab = document.querySelectorAll('.learning-type-child-tab .curriculum__tabs_child-container[data-learning-type]');
    var learningTypesTab = document.querySelectorAll('.learning-type-tab[data-learning-type]');
    for (var i = 0 ; i < learningTypeChildTab.length; i++) {

      learningTypesTab[i].classList.remove('active');
      learningTypeChildTab[i].classList.add('hide');

      if (learningTypeChildTab[i].getAttribute('data-learning-type') === learningType) {
        learningTypeChildTab[i].classList.remove('hide');
        learningTypesTab[i].classList.add('active');
        changeSwitchContainer(learningType);
        childtabChange(learningType, learningTypeChildTab[i].querySelector(".tab").getAttribute('data-learning-child-plan'));
      }
    }
  }

  function childtabChange(learningType,learningChildType){

    var learningChildTabs = document.querySelectorAll('.learning-type-child-tab .tab[data-learning-child-plan]');
    var learningTypeSubTab = document.querySelectorAll('.learning-type-sub-tab .curriculum__tabs-container[data-learning-child-plan]');
    var pricingBody = document.querySelectorAll('.curriculum__tabs-content .curriculum-learning-type-body[data-learning-child-plan]');

    for (var i = 0; i < learningChildTabs.length; i++) {
      learningChildTabs[i].classList.remove('active');
      learningTypeSubTab[i].classList.add('hide');
      pricingBody[i].classList.add('hide');

      if (learningType === learningChildTabs[i].getAttribute('data-learning-type') && learningChildType === learningChildTabs[i].getAttribute('data-learning-child-plan') ) {
        learningChildTabs[i].classList.add('active');
      }
      if (learningType === learningTypeSubTab[i].getAttribute('data-learning-type') && learningChildType === learningTypeSubTab[i].getAttribute('data-learning-child-plan')) {
        learningTypeSubTab[i].classList.remove('hide');
        tabChange(learningType, learningTypeSubTab[i].querySelector(".tab").getAttribute('data-pricing-plan'), false, learningTypeSubTab[i].querySelector(".tab").getAttribute('data-learning-child-plan'));
      }
      if (learningType === pricingBody[i].getAttribute('data-learning-type') && learningChildType === pricingBody[i].getAttribute('data-learning-child-plan')) {
        pricingBody[i].classList.remove('hide');
        
      }
    }
  }

  function tabChange(learningType, planId, scrollToSection, learningChildPlan) {
    scrollToSection = scrollToSection !== false;

    // If no grade tab available then do nothing
    const isNavGradeAvailable = document.querySelector(".learning-type-sub-tab .tab[data-pricing-plan='"+planId+"'][data-learning-type='"+learningType+"'][data-learning-child-plan='"+learningChildPlan+"']");
    if (!isNavGradeAvailable) {
      return false;
    }

    var packageTabs = document.querySelectorAll('.learning-type-sub-tab .tab[data-pricing-plan]');
    var packageTabsContext = document.querySelectorAll('.curriculum__pricing_tab_context[data-pricing-plan]');
    for (var i = 0; i < packageTabs.length; i++) {
      packageTabs[i].classList.remove('active');
      packageTabsContext[i].classList.add('hide');

      if (learningType === packageTabs[i].getAttribute('data-learning-type') && planId === packageTabs[i].getAttribute('data-pricing-plan') && learningChildPlan === packageTabs[i].getAttribute('data-learning-child-plan')) {
        packageTabs[i].classList.add('active');
      }

      if (learningType === packageTabsContext[i].getAttribute('data-learning-type') && planId === packageTabsContext[i].getAttribute('data-pricing-plan') && learningChildPlan === packageTabsContext[i].getAttribute('data-learning-child-plan')) {
        packageTabsContext[i].classList.remove('hide');
      }
    }
    // document.querySelector('.tab[data-pricing-plan=' + planId + ']').classList.add('active');
    // document.querySelector('.curriculum__pricing_tab_context[data-pricing-plan=' + planId + ']').classList.remove('hide');
    if(scrollToSection){
      let scrollElem = document.getElementById('course-pricing-start') || document.getElementById('curriculum-section');
      scrollElem.scrollIntoView();
    }
  }


  /**
